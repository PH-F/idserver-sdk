{
    "name": "xingo/idserver-sdk",
    "description": "The Xingo IDServer SDK in PHP",
    "homepage": "http://github.com/PH-F/idserver-sdk",
    "authors": [
        {
            "name": "Junior Grossi",
            "email": "juniorgro@gmail.com",
            "homepage": "http://jgrossi.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Xingo\\IDServer\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xingo\\IDServer\\ServiceProvider"
            ]
        }
    }
}
